{
  "name": "libui-napi",
  "version": "0.1.7",
  "description": "Experimental bindings for libui using n-api",
  "dependencies": {
    "node-gyp-build": "^3.3.0"
  },
  "libui": "alpha3.5-master-002",
  "devDependencies": {
    "debug": "^3.1.0",
    "home-path": "^1.0.5",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "tape": "^4.9.0",
    "tar": "^4.4.1"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "tools",
    "index.js",
    "src",
    "binding.gyp"
  ],
  "scripts": {
    "start": "node example.js",
    "download-libui": "node tools/libui-download.js",
    "clean": "rm -rf prebuilds; rm -rf build; rm *.h *.node libui.*; rm -rf node_modules;",
    "build": "node-gyp-build",
    "install": "npm run download-libui && npm run build",
    "test": "tape test.js"
  },
  "repository": "parro-it/libui-napi",
  "author": "Andrea Parodi (andrea@parro.it)",
  "license": "MIT"
}
